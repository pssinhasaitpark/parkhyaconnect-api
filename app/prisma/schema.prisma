datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") // This should match your .env file
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
  created_at DateTime @default(now())
  update_at DateTime @updatedAt
}

